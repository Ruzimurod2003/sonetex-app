<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>
        @Localizer["sonetex_title"]
    </title>
    <link rel="icon" type="image/x-icon" href="~/favicon.png" />
    <meta name="keywords" content="sonetex.uz, sonetex, product, manufacturer" />
    <meta name="description" content="This is the official website of Sonetex, where you can find all the products" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- CSS
    ============================================ -->
    <link rel="stylesheet" href="~/lib/main_template/assets/css/vendor/bootstrap.min.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/vendor/slick.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/vendor/slick-theme.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/sal.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/feather.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/fontawesome.min.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/euclid-circulara.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/swiper.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/magnify.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/odometer.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/animation.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/bootstrap-select.min.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/jquery-ui.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/magnigy-popup.min.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/plugins/plyr.css">
    <link rel="stylesheet" href="~/lib/main_template/assets/css/style.css">
</head>

<body class="rbt-header-sticky">
    <!-- Start Header Area -->
    <header class="rbt-header rbt-header-10">
        <div class="rbt-sticky-placeholder"></div>
        <!-- Start Header Top  -->
        <!-- End Header Top  -->
        <div class="rbt-header-wrapper header-space-betwween header-sticky">
            <div class="container-fluid">
                <div class="mainbar-row rbt-navigation-center align-items-center">
                    <div class="header-left rbt-header-content">
                        <div class="header-info">
                            <div class="logo">
                                <a asp-action="Index" asp-controller="Home" class="\!text-center">
                                    <img src="~/favicon.png">
                                    @Localizer["sonetex"]
                                </a>
                            </div>
                        </div>
                    </div>

                    <div class="rbt-main-navigation d-none d-xl-block">
                        <nav class="mainmenu-nav">
                            <ul class="mainmenu">
                                <li>
                                    <a asp-action="Index" asp-controller="Home">@Localizer["home_page"]</a>
                                </li>
                                <li>
                                    <a asp-action="Index" asp-controller="Manufacturers">@Localizer["manufacturers"]</a>
                                </li>
                                <li>
                                    <a asp-action="Index" asp-controller="Catalogs">@Localizer["categories"]</a>
                                </li>
                                <li>
                                    <a asp-action="Index" asp-controller="Products">@Localizer["products"]</a>
                                </li>

                                <li class="has-dropdown has-menu-child-item">
                                    <a asp-action="Index" asp-controller="Specialists">
                                        @Localizer["company"]
                                        <i class="feather-chevron-down"></i>
                                    </a>
                                    <!-- Start Mega Menu  -->
                                    <ul class="submenu">
                                        <li>
                                            <a asp-action="Index" asp-controller="Specialists">
                                                @Localizer["specialists"]
                                            </a>
                                        </li>
                                        <li>
                                            <a asp-action="Index" asp-controller="Partners">
                                                @Localizer["partners"]
                                            </a>
                                        </li>
                                        <li>
                                            <a asp-action="AboutUs" asp-controller="Home">
                                                @Localizer["our_team"]
                                            </a>
                                        </li>
                                    </ul>
                                    <!-- End Mega Menu  -->
                                </li>

                                <li>
                                    <a asp-action="ContactUs" asp-controller="Home">@Localizer["contact_with_us"]</a>
                                </li>
                            </ul>
                        </nav>
                    </div>

                    <div class="header-right">

                        <!-- Navbar Icons -->
                        <ul class="quick-access">
                            <li class="access-icon">
                                <a class="search-trigger-active rbt-round-btn" href="#">
                                    <i class="feather-search"></i>
                                </a>
                            </li>
                        </ul>

                        <div class="rbt-btn-wrapper d-none d-xl-block">
                            <div class="header-info">
                                <ul class="rbt-dropdown-menu switcher-language">
                                    <li class="has-child-menu">
                                        <a>
                                            <span class="menu-item">@Localizer["change_the_language"]</span>
                                            <i class="right-icon feather-chevron-down"></i>
                                        </a>
                                        <ul class="sub-menu">
                                            <li>
                                                <a asp-action="SetLanguage" asp-controller="Home"
                                                   asp-route-returnurl="@Context.Request.Path" asp-route-culture="en">
                                                    <img class="left-image" src="~/lib/main_template/assets/images/icons/en-us.png" alt="Language Images">
                                                    <span class="menu-item">English</span>
                                                </a>
                                            </li>
                                            <li>
                                                <a asp-action="SetLanguage" asp-controller="Home"
                                                   asp-route-returnurl="@Context.Request.Path" asp-route-culture="ru">
                                                    <img class="left-image" src="~/lib/main_template/assets/images/icons/ru.png" alt="Language Images">
                                                    <span class="menu-item">Русский</span>
                                                </a>
                                            </li>
                                            <li>
                                                <a asp-action="SetLanguage" asp-controller="Home"
                                                   asp-route-returnurl="@Context.Request.Path" asp-route-culture="uz">
                                                    <img class="left-image" src="~/lib/main_template/assets/images/icons/uz.png" alt="Language Images">
                                                    <span class="menu-item">O'zbekcha</span>
                                                </a>
                                            </li>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <!-- Start Mobile-Menu-Bar -->
                        <div class="mobile-menu-bar d-block d-xl-none">
                            <div class="hamberger">
                                <button class="hamberger-button rbt-round-btn">
                                    <i class="feather-menu"></i>
                                </button>
                            </div>
                        </div>
                        <!-- Start Mobile-Menu-Bar -->

                    </div>
                </div>
            </div>
            <!-- Start Search Dropdown  -->
            <div class="rbt-search-dropdown">
                <div class="wrapper">
                    <div class="row">
                        <div class="col-lg-12">
                            <form action="#">
                                <input type="text" placeholder="@Localizer["enter_what_you_are_looking_for"]">
                                <div class="submit-btn">
                                    <a class="rbt-btn btn-gradient btn-md" href="#">@Localizer["search"]</a>
                                </div>
                            </form>
                        </div>
                    </div>

                    <div class="rbt-separator-mid">
                        <hr class="rbt-separator m-0">
                    </div>
                </div>
            </div>
            <!-- End Search Dropdown  -->
        </div>
    </header>
    <!-- Mobile Menu Section -->
    <div class="popup-mobile-menu">
        <div class="inner-wrapper">
            <div class="inner-top">
                <div class="content">
                    <div class="logo">
                        <a asp-action="Index" asp-controller="Home" class="\!text-center">
                            <img src="~/favicon.png" />
                            @Localizer["sonetex"]
                        </a>
                    </div>
                    <div class="rbt-btn-close">
                        <button class="close-button rbt-round-btn"><i class="feather-x"></i></button>
                    </div>
                </div>
                <p class="description">@Localizer["mobile_description"]</p>
                <ul class="navbar-top-left rbt-information-list justify-content-start">
                    <li>
                        <a href="mailto:@Configuration.Email"><i class="feather-mail"></i>@Configuration.Email</a>
                    </li>
                    <li>
                        <a href="#"><i class="feather-phone"></i>@Configuration.PhoneNumber</a>
                    </li>
                </ul>
            </div>

            <nav class="mainmenu-nav">
                <ul class="mainmenu">
                    <li>
                        <a asp-action="Index" asp-controller="Home">@Localizer["home_page"]</a>
                    </li>
                    <li>
                        <a asp-action="Index" asp-controller="Manufacturers">@Localizer["manufacturers"]</a>
                    </li>
                    <li>
                        <a asp-action="Index" asp-controller="Catalogs">@Localizer["categories"]</a>
                    </li>
                    <li>
                        <a asp-action="Index" asp-controller="Products">@Localizer["products"]</a>
                    </li>

                    <li class="with-megamenu has-menu-child-item">
                        <a asp-action="Index" asp-controller="Specialists">
                            @Localizer["company"]
                            <i class="feather-chevron-down"></i>
                        </a>
                        <!-- Start Mega Menu  -->
                        <div class="rbt-megamenu grid-item-2">
                            <div class="wrapper">
                                <div class="row row--15">
                                    <div class="col-lg-12 col-xl-6 col-xxl-6 single-mega-item">
                                        <ul class="mega-menu-item">
                                            <li>
                                                <a asp-action="Index" asp-controller="Specialists">
                                                    @Localizer["specialists"]
                                                </a>
                                            </li>
                                            <li>
                                                <a asp-action="Index" asp-controller="Partners">
                                                    @Localizer["partners"]
                                                </a>
                                            </li>
                                            <li>
                                                <a asp-action="AboutUs" asp-controller="Home">
                                                    @Localizer["our_team"]
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- End Mega Menu  -->
                    </li>

                    <li>
                        <a asp-action="ContactUs" asp-controller="Home">@Localizer["contact_with_us"]</a>
                    </li>
                </ul>
            </nav>

            <div class="mobile-menu-bottom">
                <div class="rbt-btn-wrapper mb--20">
                    <div class="rbt-btn-wrapper d-none d-xl-block">
                        <div class="header-info">
                            <ul class="rbt-dropdown-menu switcher-language">
                                <li class="has-child-menu">
                                    <a>
                                        <span class="menu-item">@Localizer["change_the_language"]</span>
                                        <i class="right-icon feather-chevron-down"></i>
                                    </a>
                                    <ul class="sub-menu">
                                        <li>
                                            <a asp-action="SetLanguage" asp-controller="Home"
                                               asp-route-returnurl="@Context.Request.Path" asp-route-culture="en">
                                                <img class="left-image" src="~/lib/main_template/assets/images/icons/en-us.png" alt="Language Images">
                                                <span class="menu-item">English</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a asp-action="SetLanguage" asp-controller="Home"
                                               asp-route-returnurl="@Context.Request.Path" asp-route-culture="ru">
                                                <img class="left-image" src="~/lib/main_template/assets/images/icons/ru.png" alt="Language Images">
                                                <span class="menu-item">Русский</span>
                                            </a>
                                        </li>
                                        <li>
                                            <a asp-action="SetLanguage" asp-controller="Home"
                                               asp-route-returnurl="@Context.Request.Path" asp-route-culture="uz">
                                                <img class="left-image" src="~/lib/main_template/assets/images/icons/uz.png" alt="Language Images">
                                                <span class="menu-item">O'zbekcha</span>
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>

