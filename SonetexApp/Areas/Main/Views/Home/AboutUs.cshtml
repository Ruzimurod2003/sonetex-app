@model List<MainHomeAboutUsVM>
<div class="rbt-team-area bg-color-white rbt-section-gapBottom">
    <div class="container">
        <div class="row g-5">
            <div class="col-lg-12 mb--60">
                <div class="section-title text-center">
                    <h2 class="title">@Localizer["our_team"]</h2>
                    <p class="description mt--20">
                        @Localizer["our_team_description"]
                    </p>
                </div>
            </div>
        </div>
        <div class="row g-5">
            @foreach (var team in Model)
            {
                <!-- Start Single Team  -->
                <div class="col-lg-4 col-md-6 col-12">
                    <div class="rbt-team team-style-default style-three rbt-hover">
                        <div class="inner">
                            <div class="thumbnail">
                                <img src="~/files/@team.ImageName">
                            </div>
                            <div class="content">
                                <h2 class="title">@team.Name</h2>
                                <h6 class="subtitle theme-gradient">@team.Position</h6>
                                <span class="team-form">
                                    <i class="feather-headphones"></i>
                                    <span class="location">@team.Phone</span>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End Single Team  -->
            }
        </div>
    </div>
</div>