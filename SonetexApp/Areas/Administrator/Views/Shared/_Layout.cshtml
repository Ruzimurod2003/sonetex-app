@using Microsoft.AspNetCore.Localization
@using Microsoft.Extensions.Options
@inject IOptions<RequestLocalizationOptions> LocOptions

@{
    var requestCulture = Context.Features.Get<IRequestCultureFeature>();

    var cultureItems = LocOptions.Value.SupportedUICultures
        .Select(c => new SelectListItem { Value = c.Name, Text = c.NativeName })
        .ToList();
}

<!DOCTYPE html>
<html lang="en" dir="ltr">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>@Localizer["sonetex_title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="icon" type="image/x-icon" href="~/favicon.png" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Nunito:wght@400;500;600;700;800&display=swap" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" media="screen" href="~/lib/administrator/assets/css/perfect-scrollbar.min.css" />
    <link rel="stylesheet" type="text/css" media="screen" href="~/lib/administrator/assets/css/style.css" />
    <link defer rel="stylesheet" type="text/css" media="screen" href="~/lib/administrator/assets/css/animate.css" />
    <script src="~/lib/administrator/assets/js/perfect-scrollbar.min.js"></script>
    <script defer src="~/lib/administrator/assets/js/popper.min.js"></script>
    <script defer src="~/lib/administrator/assets/js/tippy-bundle.umd.min.js"></script>
    <script defer src="~/lib/administrator/assets/js/sweetalert.min.js"></script>
</head>

